SELECT Code,Sport,COUNT(*) AS Num_Participants
FROM EVENT INNER JOIN PARTICIPATES
ON PARTICIPATES.Event=Code
GROUP BY Code, Sport
ORDER BY Num_Participants;

SELECT ID_fac, Num_Athletes
FROM HOTEL_DELEGATION INNER JOIN DELEGATION
ON DELEGATION.Country=Delegation
WHERE (Checkin_date>to_date('05/30/2021','mm/dd/yyyy') AND Checkin_date<to_date('10/01/2021','mm/dd/yyyy'));

