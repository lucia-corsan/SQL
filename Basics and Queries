CREATE TABLE HOTEL_DELEGATION(
	Venue NUMBER NOT NULL,
	Id_Fac NUMBER NOT NULL,
    Delegation VARCHAR2(30) NOT NULL,
    Checkin_date DATE NOT NULL,
    Checkout_date DATE NOT NULL,
	constraint HOTEL_DELEGATION PRIMARY KEY (Venue,Id_fac,Delegation));
    
CREATE TABLE DELEGATION(
	Country VARCHAR2(30) NOT NULL,
    Num_athletes NUMBER NOT NULL,
	constraint DELEGATION PRIMARY KEY (Country));

CREATE TABLE ATHLETE(
    ID_number NUMBER NOT NULL,
    Name_at VARCHAR2(30) NOT NULL,
    Surnames_at VARCHAR2(30) NOT NULL,
    Date_birth DATE NOT NULL,
    Sport VARCHAR2(30) NOT NULL,
    Food_type VARCHAR2(30) NOT NULL,
    Height NUMBER NOT NULL,
    Weight NUMBER NOT NULL,
    Phone NUMBER NOT NULL,
    Email VARCHAR2(30) NULL,
    Delegation VARCHAR2(30) NOT NULL,
    constraint ATHLETE PRIMARY KEY(ID_number));
    
CREATE TABLE EVENT(
	Code NUMBER NOT NULL,
    Sport VARCHAR2(30) NOT NULL,
    Sport_complex VARCHAR2(30) NOT NULL,
    Date_event DATE NOT NULL,
    Duration_min NUMBER NOT NULL,
    N_part NUMBER NOT NULL,
    N_officials NUMBER NOT NULL,
	constraint EVENT PRIMARY KEY (Code));
  
CREATE TABLE PARTICIPATES(
	Event NUMBER NOT NULL,
    Athlete NUMBER NOT NULL,
    Date_part DATE NOT NULL,
	constraint PARTICIPATES PRIMARY KEY(Event,Athlete));
    
CREATE TABLE ATTENDS(
	Official NUMBER NOT NULL,
    Event NUMBER NOT NULL,
    Date_att DATE NOT NULL,
    Role_att VARCHAR2(30) NOT NULL,
	constraint ATTENDS PRIMARY KEY(Official,Event));
    
CREATE TABLE EQUIPMENT(
    Event NUMBER NOT NULL,
    Equipement VARCHAR2(30) NOT NULL,
    constraint EQUIPEMENT PRIMARY KEY(Event));

CREATE TABLE VENUE(
    Code NUMBER NOT NULL,
    Address VARCHAR2(30) NOT NULL,
    constraint VENUE PRIMARY KEY(Code));
    
CREATE TABLE SPORT_COMPLEX(
	Name_com VARCHAR2(30) NOT NULL,
    Opening_hours NUMBER NOT NULL,
    Address VARCHAR2(30) NOT NULL,
    Manager_com NUMBER NOT NULL,
    Venue NUMBER NOT NULL,
    Complex_type VARCHAR2(30) NOT NULL,
	CONSTRAINT COMPLEX_TYPE CHECK (Complex_type IN ('Multisport', 'Onesport')),
	constraint SPORT_COMPLEX PRIMARY KEY (Name_com));

CREATE TABLE MULTI_COMPLEX(
	Name_com VARCHAR2(30) NOT NULL,
    Area NUMBER NOT NULL,
    Num_sports NUMBER NOT NULL,
    Num_outdoor NUMBER NOT NULL,
    Num_indoor NUMBER NOT NULL,
	constraint MULTI_COMPLEX PRIMARY KEY (Name_com));

CREATE TABLE EMPLOYEE(
    ID_number NUMBER NOT NULL,
    Name_em VARCHAR2(30) NOT NULL,
    Surnames_em VARCHAR2(30) NOT NULL,
    Email VARCHAR2(30) NULL,
    Employee_Type VARCHAR2(30) NOT NULL,
    CONSTRAINT EMPLOYEE_TYPE CHECK (Employee_Type IN ('Manager', 'Supervisor', 'Maintenance', 'Cleaning', 'Security','Official')),
    constraint EMPLOYEE PRIMARY KEY(ID_number));
    
CREATE TABLE FACILITY_STAFF(
	Venue NUMBER NOT NULL,
	Id_Fac NUMBER NOT NULL,
    Employee NUMBER NOT NULL,
    constraint FACILITY_STAFF PRIMARY KEY(Venue, ID_fac,Employee));
    
CREATE TABLE RESTAURANT(
	Venue NUMBER NOT NULL,
	Id_Fac NUMBER NOT NULL,
    Food_type VARCHAR2(30) NOT NULL,
    constraint RESTAURANT PRIMARY KEY(Venue, ID_fac,Food_type));

CREATE TABLE EMP_PHONE(
    ID_number NUMBER NOT NULL,
    Phone NUMBER NOT NULL,
    constraint EMP_PHONE PRIMARY KEY(ID_number, Phone));
    
CREATE TABLE FACILITY(
    Venue NUMBER NOT NULL,
    ID_fac NUMBER NOT NULL,
    Capacity_fac NUMBER NOT NULL,
    Manager_id NUMBER NOT NULL,
    Facility_Type VARCHAR2(30) NOT NULL,
    CONSTRAINT FACILITY_TYPE CHECK (Facility_type IN ('Hotel', 'Apartment', 'Restaurant')),
    constraint FACILITY PRIMARY KEY(Venue,ID_fac));
    
 ALTER TABLE HOTEL_DELEGATION ADD CONSTRAINT HOTEL_DELEGATION_fk1 FOREIGN
KEY (Delegation) REFERENCES DELEGATION(Country);

ALTER TABLE HOTEL_DELEGATION ADD CONSTRAINT HOTEL_DELEGATION_fk0 FOREIGN
KEY (Venue,ID_fac) REFERENCES FACILITY(Venue,ID_fac);

ALTER TABLE ATHLETE ADD CONSTRAINT ATHLETE_fk0 FOREIGN
KEY (Delegation) REFERENCES DELEGATION(Country);

ALTER TABLE PARTICIPATES ADD CONSTRAINT PARTICIPATES_fk0 FOREIGN
KEY (Athlete) REFERENCES ATHLETE(ID_Number);

ALTER TABLE PARTICIPATES ADD CONSTRAINT PARTICIPATES_fk1 FOREIGN
KEY (Event) REFERENCES EVENT(Code);

ALTER TABLE ATTENDS ADD CONSTRAINT ATTENDS_fk0 FOREIGN
KEY (Official) REFERENCES EMPLOYEE(ID_Number);

ALTER TABLE ATTENDS ADD CONSTRAINT ATTENDS_fk1 FOREIGN
KEY (Event) REFERENCES EVENT(Code);

ALTER TABLE EQUIPMENT ADD CONSTRAINT EQUIPMENT_fk0 FOREIGN
KEY (Event) REFERENCES EVENT(Code);

ALTER TABLE EVENT ADD CONSTRAINT EVENT_fk0 FOREIGN
KEY (Sport_complex) REFERENCES SPORT_COMPLEX(Name_com);

ALTER TABLE SPORT_COMPLEX ADD CONSTRAINT SC_fk0 FOREIGN
KEY (Manager_com) REFERENCES EMPLOYEE(ID_Number);

ALTER TABLE SPORT_COMPLEX ADD CONSTRAINT SC_fk1 FOREIGN
KEY (Venue) REFERENCES VENUE(Code);

ALTER TABLE MULTI_COMPLEX ADD CONSTRAINT MC_fk0 FOREIGN
KEY (Name_com) REFERENCES SPORT_COMPLEX(Name_com);

ALTER TABLE EMP_PHONE ADD CONSTRAINT EMPH_fk0 FOREIGN
KEY (ID_number) REFERENCES EMPLOYEE(ID_Number);

ALTER TABLE FACILITY_STAFF ADD CONSTRAINT FAC_STAFF_fk0 FOREIGN
KEY (Venue,ID_fac) REFERENCES FACILITY(Venue,ID_fac);

ALTER TABLE FACILITY_STAFF ADD CONSTRAINT FAC_STAFF_fk1 FOREIGN
KEY (Employee) REFERENCES EMPLOYEE(ID_Number);

ALTER TABLE FACILITY ADD CONSTRAINT FACILITY_fk0 FOREIGN
KEY (Venue) REFERENCES Venue(Code);

ALTER TABLE FACILITY ADD CONSTRAINT FACILITY_fk1 FOREIGN
KEY (Manager_id) REFERENCES EMPLOYEE(ID_Number);

ALTER TABLE RESTAURANT ADD CONSTRAINT RESTAURANT_fk0 FOREIGN
KEY (Venue,ID_fac) REFERENCES FACILITY(Venue,ID_fac);

